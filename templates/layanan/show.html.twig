{% extends 'base.html.twig' %}

{% block title %}Layanan{% endblock %}

{% block body %}
    <h1>Layanan</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ layanan.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ layanan.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ layanan.slug }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ layanan.description }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ layanan.image }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ layanan.price }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ layanan.created ? layanan.created|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ layanan.updated ? layanan.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ContentChanged</th>
                <td>{{ layanan.contentChanged ? layanan.contentChanged|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('layanan_index') }}">back to list</a>

    <a href="{{ path('layanan_edit', {'id': layanan.id}) }}">edit</a>

    {{ include('layanan/_delete_form.html.twig') }}
{% endblock %}
